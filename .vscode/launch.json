    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${fileDirname}",
            "args": ["-h"]
        },
        {
            "name": "rip AON",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/PFSpellbook/PFSpellbook.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/PFSpellbook/",
            "args": ["-r", "aon", "-s"]
        }
    ]
}